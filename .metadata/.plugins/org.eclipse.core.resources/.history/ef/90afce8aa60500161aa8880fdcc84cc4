#include "BusStop.h"

BusStop::BusStop(int ID, string name, Localization localization){
	this->ID = ID;
	this->name = name;
	this->localization = localization;
}

void setID(int ID);
int getID();
void setName(string name);
string getName();
void setLocalization(Localization localization);
Localization getLocalization();
