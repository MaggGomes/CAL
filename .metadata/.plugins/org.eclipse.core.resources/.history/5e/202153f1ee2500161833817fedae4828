#include "SchoolBus.h"

using namespace std;
int main(){
	vector <RoadT *> a;
	SchoolBus schoolBus;
	schoolBus.getRoadMap().insert(make_pair("abc", a));
	if (schoolBus.getRoadMap().count("abc") > 0)
		cout << "sim";
	else
		cout << "nao";
	pressKeyToContinue();
	schoolBus.menuStarting();

	return 0;
}
