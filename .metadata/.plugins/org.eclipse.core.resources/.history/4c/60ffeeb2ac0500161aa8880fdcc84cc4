#ifndef SCHOOLBUS_H
#define SCHOOLBUS_H

#include <string>

using namespace std;

class SchoolBus {
	vector <Bus>
public:
	Bus(int buildYear, string registration, int capacity);
	void setBuildYear(int year);
	int getBuildYear();
	void setRegistration(string registration);
	string getRegistration();
	void setCapacity(int capacity);
	int getCapacity();
};

#endif // SCHOOLBUS_H
