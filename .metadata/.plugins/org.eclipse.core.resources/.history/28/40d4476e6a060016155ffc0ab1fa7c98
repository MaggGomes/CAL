#include "SchoolBus.h"
#include "Graphviewer.h"

using namespace std;

int main(){
	GraphViewer *gv = new GraphViewer(600, 600, true);
	//gv->setBackground("city.jpg");
	gv->createWindow(600, 600);
	gv->addNode(0, 200, 200);
	gv->rearrange();

	SchoolBus schoolBus;
	schoolBus.menuStarting();


	return 0;
}
