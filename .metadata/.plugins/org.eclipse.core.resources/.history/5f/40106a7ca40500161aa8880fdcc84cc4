#include "Student.h"

Student::Student(string name, int ID, int age){
	this->name = name;
	this->age = age;
}

void Student::setName(string name){
	this->name = name;
}

string Student::getName(){
	return name;
}

void setID(int ID);
	int getID();

void Student::setAge(int age){
	this->age = age;
}

int Student::getAge(){
	return age;
}
