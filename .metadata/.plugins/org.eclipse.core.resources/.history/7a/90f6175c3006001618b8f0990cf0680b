#include "Student.h"

Student::Student(string name, int ID, const BusStop stop, const School school){
	this->name = name;
	this->ID = ID;
	this->stop = stop;
	this->school = school;
}

void Student::setName(string name){
	this->name = name;
}

string Student::getName(){
	return name;
}

void Student::setID(int ID){
	this->ID = ID;
}

int Student::getID(){
	return ID;
}

void setBusStop(const BusStop stop);
BusStop getBusStop();
void setSchool(const School school);
School getSchool();
